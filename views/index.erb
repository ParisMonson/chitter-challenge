<!DOCTYPE html>

<html>
  <head>
    <title>Welcome to Chitter!</title>
    <a href='/signup'>Sign Up Here</a>
  </head>

  <body>
  <h1>Chitter Message Board</h1>
    <%@peeps.each do |peep|%>
    <div>
      <head>
        Peep ID#<%=peep.id%>
      </head>
      <p>
        <%=peep.content%>
      </p>
      <footer>
        <%=@users.find(peep.user_id)[0].username%>    <%=@users.find(peep.user_id)[0].email%>    <%=peep.date%>
      </footer>
    </div>
    <%end%>
  </body>

</html>